(filternetwork play "play files with echo-effect"
	(node rf read_file
		(export-param filename filename "file to be played")
	)
	(node aout audio_out)
	(node mix mix)
	(connect rf out mix in)
	(connect rf out mix in)
	(node delay delay
		(set-param delay 200))
	(node mix2 mix)
	(node dup dup)
	(connect mix out mix2 in)
	(connect mix2 out dup in)
	(connect dup out1 aout in)
	(connect dup out2 delay in)
	(connect delay out mix2 in
		(set-destparam gain 0.7))
)
