(filternetwork test_io "Read a file, apply echo, save it"
	(node rf read_file
		(export-param filename infile "infile")
	)
	(node wf write_file
		(export-param filename outfile "outfile")
	)
	(node el echo)
	(node er echo)
	(connect rf out el in)
	(connect rf out er in)
	(connect el out wf in)
	(connect er out wf in)
)
