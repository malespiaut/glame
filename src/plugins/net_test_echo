(filternetwork play "play files with echo-effect"
	(node rf read_file
		(export-param filename filename "file to be played")
	)
	(node mix mix)
	(node echo echo
		(set-param time 0.2)
		(set-param mix 0.9))
	(node aout oss_audio_out)
	(connect rf out mix in)
	(connect rf out mix in)
	(connect mix out echo in)
	(connect echo out aout in)
)
